{
  "node": true,
  "browser": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "regexp": true,
  "undef": true,
  "unused": true,
  "strict": false,
  "forin": false,
  "freeze": true,
  "noempty": true,
  "nonbsp": true,
  "plusplus": false,
  "maxparams": 10,
  "maxdepth": 5,
  "maxstatements": 15,
  "maxcomplexity": 6,
  "maxlen": 150,
  "globals": {
    /* Test */
    "jasmine": true,
    "describe": true,
    "element": true,
    "browser": true,
    "beforeEach": true,
    "afterEach": true,
    "expect": true,
    "it": true,
    "by": true,
    /* Custom */
    "ngRoutes": false,
    "ngSanitize": false,
    "ngCookies": false,
    "ngResource": false
  },
  "predef": ["angular"]
}